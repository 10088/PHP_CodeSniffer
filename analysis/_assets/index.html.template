<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <title>((title))</title>
    <link rel="shortcut icon" type="image/png"  href="((assetPath))_assets/favicon.png">
    <link rel="stylesheet" type="text/css" href="((assetPath))_assets/styles.css" />
    <script src="((assetPath))_assets/Chart.min.js"></script>
    <script src="((assetPath))_assets/scripts.js"></script>
</head>
<body>
<div id="gradeInstructionsWrap" class="listBoxWrap">
    <div class="listBoxContent">
        <div class="listBoxClose" onclick="toggleGradeBox();"></div>
        <div class="listBoxHeader">
            <h2>Embed Instructions</h2>
            <img src="((reportURL))/grade.svg" title="Code consistency">
        </div>
        <div class="listBoxListWrap">
            <ul class="listBoxList">
                <li>
                    <div class="td1">Image URL</div>
                    <div class="td2">((reportURL))/grade.svg</div>
                </li>
                <li>
                    <div class="td1">Markdown</div>
                    <div class="td2">[![Code consistency](((reportURL))/grade.svg)](((reportURL)))</div>
                </li>
            </ul>
        </div>
    </div>
</div>

<div id="listBoxWrap" class="listBoxWrap"></div>

<div id="consistency" class="((gradeColour))" title="((gradeDescription))" onclick="toggleGradeBox();">
    <span class="grade">((grade))</span>consistency
</div>

<div id="mainWrap" class="mainWrap">
    <div id="introductionWrap" class="introductionWrap">
        <div class="introduction">
            <div class="icon"></div>
            <h1>((heading))</h1>
            ((intro))
        </div>
    </div>

    <div id="flyoutFixed" class="flyoutFixed">
        <div id="flyoutControl" class="flyoutControl"></div>
        <div id="flyoutPin" class="flyoutPin" title="Pin sidebar open"></div>
        <div class="flyoutHeader">
            <h1>Conventions</h1>
        </div>
    </div>
    <div id="flyout" class="flyout">
        <ul class="flyoutList">
            ((sidebar))
        </ul>
    </div>

    <div id="fixedHeader" class="fixedHeader">
        <div id="fixedHeaderContent" class="introduction">
            <div class="icon"></div>
            <h1>((heading))</h1>
             <a href="#" title="Return to the top of the report" class="backToTop"><span class="screenHide">Back to top</span></a>
        </div>
    </div>

    <div id="contentWrap" class="contentWrap">
    ((html))
    </div>

    <div class="footer">((footer))</div>
</div>

<script>
init();
